{
    "name": "contao-community-alliance/github-payload",
    "description": "Parse the github API payload from a webhook.",
    "keywords": [
        "github",
        "webhook"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Lins",
            "email": "tristan.lins@bit3.de",
            "homepage": "http://bit3.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@contao-community-alliance.de",
        "issues": "https://github.com/contao-community-alliance/github-payload/issues",
        "wiki": "https://github.com/contao-community-alliance/github-payload/wiki",
        "irc": "irc://irc.freenode.org/contao.cca",
        "source": "https://github.com/contao-community-alliance/github-payload"
    },
    "require": {
        "php": ">=5.3",
        "jms/serializer": "~0.0"
    },
    "require-dev": {
        "symfony/http-foundation": "~2.0",
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~1.0",
        "phpmd/phpmd": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "ContaoCommunityAlliance\\GithubPayload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContaoCommunityAlliance\\GithubPayload\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    }
}
